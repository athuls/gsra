Location : varchar(30)
Latitude : double
Longitude : double
Length : double
Width : double
Year Built : number 
Year Reconstructed : number
Inspection Frequency : number
Bridge Type : varchar(6)
Inspection Key : varchar(4)
Scour Evaluation : varchar(1)
Number of Pins : number int
DOT Region : varchar(10)
